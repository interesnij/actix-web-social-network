<div class="media">
  <% let photo_creator = object.get_creator(); %>
  <a href="<%= photo_creator.get_link() %>" class="ajax">
    <figure>
      <%- photo_creator.get_50_avatar() %>
    </figure>
  </a>
    <div class="media-body">
        <h6 class="mb-0">
          <a href="<%= photo_creator.get_link() %>" class="ajax">
            <%= photo_creator.get_full_name() %>
          </a>
        </h6>
        <p class="mb-0">
          <%= object.created.format("%d-%m-%Y Ð² %H:%M").to_string() %>
        </p>
    </div>
</div>
