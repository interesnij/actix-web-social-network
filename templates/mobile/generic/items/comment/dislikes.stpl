  <span><%= text %> <%= item.disliked %> <%= item.count_dislikes_ru() %></span>
  <br><br>
  <span style="display: flex;">
    <% for voter in object_list.iter() { %>
      <% include!("../user/voter.stpl"); %>
    <% } %>
  </span>
</div>
<% } %>
