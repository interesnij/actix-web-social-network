
<div class="main-container mt-3 pk_saver" data-pk="{{ community.pk }}" data-type="community_survey">
    <div class="input-group mb-3">
        <input type="text" class="form-control form-control-sm" placeholder="Поиск опросов">
        <div class="input-group-append">
            <button class="btn btn-sm border" type="button">Искать</button>
        </div>
    </div>
    <% include!("admin_lists.stpl"); %>

    <span class="load_block">

      <div class="media mt-3" style="padding-left: 7px;">
        <h5 class="mt-1" class="uuid_saver" data-uuid="<% list.id %>" data-pk="<% list.id %>">
          <span class="second_list_name" data-name="<%= list.name %>"><%= list.name %></span> (<%= list.count_items() %>)
          {% if is_user_can_create_surveys %}
          <a class="pointer add_survey" tooltip="Добавить опрос" flow="up">
            <svg class="svg_info" fill="currentColor" viewBox="0 0 24 24"><path d="M19 13h-6v6h-2v-6H5v-2h6V5h2v6h6v2z"/><path d="M0 0h24v24H0z" fill="none"/></svg>
          </a>
          {% endif %}
          <% include!("../../../generic/items/list_meta.stpl"); %>
        </h5>
      </div>
    <div class="card mb-2">
        <div class="card-body">
            <ul class="is_paginate drag_container is_stat_list" data-pk="<% list.id %>" data-type="survey" data-community-id="<%= list.community_id.unwrap() %>">

              <% if object_list.len() > 0 { %>
                <% for object in object_list.iter() { %>
                  <% include!("../survey.stpl"); %>
                {% endfor %}
                <% if next_page_number > 0 { %>
                <div class="load centered m-1 next_page_list" data-link="/community/<%= community.id %>/survey_list/<%= list.id %>/?page=<%= next_page_number %>">
                    <img src="/static/images/preloader.gif">
                </div>
                {% endif %}

                {% else %}
                  <% include!("../../../generic/items/no_surveys.stpl"); %>
                {% endif %}
            </ul>
        </div>
    </div>
    </span>
</div>
