
<div class="main-container mt-3" data-pk="<% list.id %>" data-type="community_survey_list">
    <div class="input-group mb-3">
        <input type="text" class="form-control form-control-sm" placeholder="Поиск опросов">
        <div class="input-group-append">
            <button class="btn btn-sm border" type="button">Искать</button>
        </div>
    </div>
    <span class="load_block">

    <div class="media mt-3 uuid_saver" data-pk="<% list.id %>" data-uuid="<% list.id %>" style="padding-left: 7px;">
      <h5 class="mt-1">
        <span class="second_list_name" data-name="<%= list.name %>"><%= list.name %></span> (<%= list.count_items() %>)
        <span class="pointer badge badge-light-primary copy_link">Копировать ссылку</span>
      </h5>
    </div>
    <div class="card mb-2">
        <div class="card-body">
            <ul class="is_paginate is_stat_list" data-pk="<% list.id %>" data-type="<% if list.community_id.is_some() { %>community<% } else { %>user<% } %>_survey_list">

              <% if object_list.len() > 0 { %>
                <% for object in object_list.iter() { %>
                  <% include!("../anon_survey.stpl"); %>
                {% endfor %}
                <% if next_page_number > 0 { %>
                <div class="load centered m-1 next_page_list" data-link="/community/<%= community.id %>/survey_list/<%= list.id %>/?page=<%= next_page_number %>">
                    <img src="/static/images/preloader.gif">
                </div>
                {% endif %}

                {% else %}
                  <% include!("../no_items.stpl"); %>
                {% endif %}
            </ul>
        </div>
    </div>
    </span>
</div>
