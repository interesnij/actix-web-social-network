<% if object_list.len() > 0 { %>
  <% for object in object_list.iter() { %>
    <% include!("../good.stpl"); %>
<% } %>
<% if next_page_number > 0 { %>
<div class="load centered m-1 next_page_list" data-link="/community/<%= community.id %>/goods_list/<%= list.id %>/?page=<%= next_page_number %>">
    <img src="/static/images/preloader.gif">
</div>
<% } %>

<% } else { %>
  <% include!("../no_goods.stpl"); %>
<% } %>
