
<div class="main-container mt-3 pk_saver" data-pk="{{ community.pk }}" data-type="community_music">
{% if is_user_can_see_music_section %}
          <div class="input-group mb-3">
            <input type="text" class="form-control form-control-sm" placeholder="Поиск музыки">
            <div class="input-group-append">
              <button class="btn btn-sm border" type="button">Искать</button>
            </div>
          </div>
          <% include!("lists.stpl"); %>
          <span class="load_block">

          <div class="media mt-3 uuid_saver" data-pk="<% list.id %>" data-uuid="<% list.id %>" style="padding-left: 7px;">
            <h5 class="mt-1">
              <span class="second_list_name" data-name="<%= list.name %>"><%= list.name %></span> (<%= list.count_items() %>)
              <span class="pointer badge badge-light-primary copy_link">Копировать ссылку</span>
            </h5>
          </div>
          <div class="card mb-2">
              <div class="card-body">
                <% if object_list.len() > 0 { %>
                    <ul class="is_paginate is_stat_list" data-pk="<%= list.id %>" data-type="mus" data-community-id="<%= list.community_id.unwrap() %>">
                      <% for object in object_list.iter() { %>
                        <% include!("../anon_track.stpl"); %>
                      {% endfor %}
                      <% if next_page_number > 0 { %>
                      <div class="load centered m-1 next_page_list" data-link="/community/<%= community.id %>/music_list/<%= list.id %>/?page=<%= next_page_number %>">
                          <img src="/static/images/preloader.gif">
                      </div>
                      {% endif %}
                    </ul>

                {% else %}
                  <% include!("../no_tracks.stpl"); %>
                {% endif %}
              </div>
          </div>
      </span>
      {% else %}
        <% include!("../../../generic/centered/private_page.stpl"); %>
      {% endif %}
</div>
