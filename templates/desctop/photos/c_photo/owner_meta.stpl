<div class="media">
  <% let photo_community = object.get_community(); %>
  <a href="<%= photo_community.link %>" class="ajax">
    <figure>
      <%- photo_community.get_50_avatar() %>
    </figure>
  </a>
    <div class="media-body">
        <h6 class="mb-0">
          <a href="<%= photo_community.link %>" class="ajax">
            <%= photo_community.name %>
          </a>
        </h6>
        <p class="mb-0">
          <%= object.created.format("%d-%m-%Y Ð² %H:%M").to_string() %>
        </p>
    </div>
</div>
